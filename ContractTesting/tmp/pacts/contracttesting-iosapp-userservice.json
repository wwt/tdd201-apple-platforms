{
  "consumer": {
    "name": "ContractTesting-iOSApp"
  },
  "provider": {
    "name": "UserService"
  },
  "interactions": [
    {
      "description": "A request for that user",
      "providerState": "A user with id 1 exists",
      "request": {
        "method": "get",
        "path": "/users/1"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "age": 48,
          "name": "Otto",
          "email": "jordonupton3422@littel.com"
        },
        "matchingRules": {
          "$.body.age": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.email": {
            "match": "regex",
            "regex": "(.*)@(.*)"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}